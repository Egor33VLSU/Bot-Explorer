{
  "name": "Bot Explorer",
  "description": "Extension to help user explore bot activity",
  "version": "1.0",
  "manifest_version": 3,
 "background": {
    "service_worker": "background.js"
  },

"permissions": [
    "tabs"
],

  "host_permissions": [
        "https://vk.com/*",
         "https://api.vk.com/*",
"https://oauth.vk.com/*",
"https://*.userapi.com/*",
"https://login.vk.com/*"
    ],

    





    
 



"action": {
 "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started145.png",
      "32": "/images/get_started145.png",
      "48": "/images/get_started145.png",
      "128": "/images/get_started145.png"
      
    }
  },
  "icons": {
    "16": "/images/get_started145.png",
    "32": "/images/get_started145.png",
    "48": "/images/get_started145.png",
    "128": "/images/get_started145.png"
    
  }
}

